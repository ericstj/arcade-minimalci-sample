<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>netcoreapp2.0;netcoreapp3.0</TargetFrameworks>
    <IsPackable>true</IsPackable>
    <AssemblyName>HelloWorld</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="HelloWorld_Cpp.vcxproj">
        <ExcludeFromPublishPackageReferences>true</ExcludeFromPublishPackageReferences>
    </ProjectReference>
  </ItemGroup>
</Project>
